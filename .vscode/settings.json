{
    "editor.suggestSelection": "first",
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter.notebook.ipynb"
    },
    "git.suggestSmartCommit": false,
    "git.autofetch": true,
    "typescript.updateImportsOnFileMove.enabled": "never",
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "editor.wordWrap": "on",
    "liveServer.settings.donotShowInfoMsg": true,
    "editor.linkedEditing": true,
    "editor.defaultFormatter": "SimonSiefke.prettier-vscode",
    "files.autoSave": "afterDelay",
    "php.validate.executablePath": "/Applications/MAMP/bin/php/php8.x.x/bin/php",
    "liveSassCompile.settings.includeItems": [],
    "prettier.bracketSameLine": true,
    "prettier.requireConfig": true,
    "prettier.jsxSingleQuote": true,
    "prettier.printWidth": 100,
    "prettier.tabWidth": 2,
    "prettier.trailingComma": "es5",
    "cSpell.languageSettings": [],
    "reactSnippets.settings.prettierEnabled": true,
    "prettier.enableDebugLogs": true,
    "prettier.insertPragma": true,
    "prettier.requirePragma": true,
    "prettier.resolveGlobalModules": true,
    "errorLens.onSave": true,
    "editor.formatOnSave": true,
    "editor.fontLigatures": false,
    "material-icon-theme.folders.theme": "specific",
    "cursor.cpp.disabledLanguages": [
        "plaintext",
        "markdown",
        "scminput"
    ],
    "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
    "editor.guides.bracketPairs": "active",
    "window.zoomLevel": -1,
    "workbench.iconTheme": "vscode-icons",
    "editor.inlayHints.fontSize": 1,
    "editor.inlayHints.fontFamily": "Menlo, Monaco, 'Courier New', monospace",
    "editor.formatOnSaveMode": "modifications",
    "javascript.updateImportsOnFileMove.enabled": "always",
    "workbench.preferredHighContrastColorTheme": "Material Theme Ocean",
    "workbench.colorTheme": "Default Dark Modern",
    "gitlens.views.commitDetails.files.layout": "list",
    "typescript.inlayHints.parameterNames.enabled": "all",
    "typescript.inlayHints.functionLikeReturnTypes.enabled": true,
}